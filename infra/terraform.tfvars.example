# Terraform configuration example
# Copy this file as terraform.tfvars and fill in values

project_id   = "your-gcp-project-id"
region       = "europe-west1"
zone         = "europe-west1-a"
github_owner = "Replikanti"
github_repo  = "ralph-platform"
github_token = "ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxx"

# Application secrets (optional - leave empty to use placeholders)
# You can update these values later in GCP Secret Manager
anthropic_api_key     = "sk-ant-xxxxx"  # or leave empty
langfuse_public_key   = "pk-lf-xxxxx"   # or leave empty
langfuse_secret_key   = "sk-lf-xxxxx"   # or leave empty
linear_webhook_secret = "xxxxx"         # or leave empty
linear_api_key        = "lin_api_xxxxx" # or leave empty
